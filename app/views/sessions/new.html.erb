<div class="session-wrap">
  <div class="login-header">
    <h1>Login</h1> </br>

    <div class="text">
      <%= form_tag '/login' do %>
        Email: <%= text_field_tag :email, "", class: 'form-group form-control', type: 'email' %>
        Password: <%= password_field_tag :password, "", class: 'form-group form-control', type: 'password' %>
        <%= submit_tag "Submit", class: 'btn btn-secondary' %>
        <%= link_to "Signup", signup_path, class: 'signup' %>
      <% end %>
    </div>
  </div>
</div>
