<div class="login-header">
  <h1>Login</h1> </br>

  <%= form_tag '/login' do %>

    <form>
    <div class="text">
      Email: <%= text_field_tag :email, "", class: 'form-group', type: 'email' %> </br>
      Password: <%= password_field_tag :password, "", class: 'form-group', type: 'password' %> </br>
      <%= submit_tag "Submit" %>
    </div>
</div>
<% end %>
