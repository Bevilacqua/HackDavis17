<!DOCTYPE html>
<html>
  <head>
    <title>HackDavis17</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
    <% if notice %>
    	<div class="alert alert-success alert-dismissable">
    	  <a href="#" class="close" data-dismiss="alert" id="notice" aria-label="close">&times;</a>
    	  <strong>AYYYY!</strong> <%= notice %>
    	</div>
    <% end %>
    <%= yield %>
  </body>
</html>
