<p id="notice"><%= notice %></p>

<h2>Create a new group.</h2>
<%= form_for @group do |f| %>
  <%= f.label :name %>:
  <%= f.text_field :name %><br />
  <%= f.hidden_field :user_id, :value => @user.id %>

  <%= f.submit %>
<% end %>

<%= link_to 'Edit', edit_user_path(@user) %> |
<%= link_to 'Back', users_path %>
